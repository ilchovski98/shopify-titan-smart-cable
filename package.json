{
  "name": "shopify-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel \"webpack:dev\" \"shopify\"",
    "start:prod": "npm-run-all --parallel \"webpack:prod\" \"shopify\"",
    "shopify": "theme watch",
    "download": "theme download -e live",
    "webpack:dev": "webpack --watch --config ./config/webpack.config.js --mode=development",
    "webpack:prod": "webpack --watch --config ./config/webpack.config.js --mode=production",
    "deploy": "webpack --config ./config/webpack.config.js --mode=production && theme deploy",
    "deploy:dev": "webpack --config ./config/webpack.config.js --mode=development && theme deploy",
    "deploy:qa": "webpack --config ./config/webpack.config.js --mode=production && theme deploy -e qa",
    "deploy:live": "webpack --config ./config/webpack.config.js --mode=production && theme deploy -e live --allow-live"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^4.1.0",
    "colors": "^1.4.0",
    "css-loader": "^4.2.2",
    "cypress": "^9.3.1",
    "file-loader": "^6.0.0",
    "inquirer": "^7.3.3",
    "jquery": "^3.5.1",
    "js-yaml": "^3.14.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "yaml": "^1.10.0",
    "yargs": "^15.4.1"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "dependencies": {
    "slick-carousel": "^1.8.1"
  }
}
